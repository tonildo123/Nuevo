<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.sergio.nuevo.presentacion.vistas.ResultadosLiquidacionesImgImpl">

    <!-- TODO: Update blank fragment layout --><![CDATA[
    android:layout_height="match_parent"/>

    ]]>

    <GridView
        android:id="@+id/gridView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/FABSalir"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="top|center_horizontal"
        app:fab_icon="@drawable/exit_to_app"
        app:layout_anchor="@+id/gridView"
        app:layout_anchorGravity="bottom|right"
        fab:fab_colorNormal="@color/blue700"
        fab:fab_title="Ver Resultados" />

</android.support.design.widget.CoordinatorLayout>
